{
  "name": "graphql-ts",
  "version": "1.0.0",
  "description": "server for services.eddienaff.dev and portfolio projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "npm run build && env NODE_ENV=production node -r dotenv/config ./dist/index.js",
    "start:dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "db:setup": "ts-node ./src/test-utils/setup.ts",
    "test": "npm run db:setup && TERM=dumb jest --detectOpenHandles",
    "tdev": "npm run db:setup && TERM=dumb jest --watch --detectOpenHandles",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f src/config/prod-orm-config.ts",
    "typeorm:cli:d": "ts-node ./node_modules/typeorm/cli -f src/config/dev-orm-config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "0.0.13",
    "@types/date-fns": "^2.6.0",
    "@types/dlv": "^1.1.2",
    "@types/dotenv": "^8.2.0",
    "@types/express-session": "^1.17.0",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@types/nodemailer": "^6.4.0",
    "@types/pino": "^5.17.0",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.13.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.11.0",
    "aws-sdk": "^2.640.0",
    "bcryptjs": "^2.4.3",
    "bufferutil": "^4.0.1",
    "canvas": "^2.5.0",
    "casual": "^1.6.2",
    "colors": "^1.4.0",
    "connect-redis": "^4.0.4",
    "dataloader": "^2.0.0",
    "date-fns": "^2.11.1",
    "dlv": "^1.1.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^14.6.0",
    "graphql-redis-subscriptions": "^2.2.1",
    "graphql-subscriptions": "1.1.0",
    "internal-ip": "^6.0.0",
    "ioredis": "^4.16.0",
    "jest": "^25.1.0",
    "nodemailer": "^6.4.16",
    "pg": "^7.18.2",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.1",
    "postmark": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.24",
    "utf-8-validate": "^5.0.2",
    "uuid": "^7.0.2"
  }
